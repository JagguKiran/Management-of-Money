            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".BalanceAct"
            android:label="@string/bal"
            android:parentActivityName=".SourceMainAct"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".IncomeDisplay"
            android:label="@string/incomeDisplay"
            android:parentActivityName=".IncomeAct"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ExpenseDisplay"
            android:label="@string/expDisplay"
            android:parentActivityName=".ExpenseAct"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CardDisplay"
            android:label="@string/cD"
