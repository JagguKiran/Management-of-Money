        <activity
            android:name=".IncomeAct"
            android:label="@string/income"
            android:parentActivityName=".SourceMainAct"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ExpenseAct"
            android:label="@string/exp"
            android:parentActivityName=".SourceMainAct"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CardAct"
            android:label="@string/card"
            android:parentActivityName=".SourceMainAct"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SettingAct"
            android:label="@string/set"
