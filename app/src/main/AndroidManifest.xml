            android:label="@string/pieChart"
            android:parentActivityName=".ReportAct"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".BarAct"
            android:label="@string/barChart"
            android:parentActivityName=".ReportAct"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".BarChartNavigationAct"
            android:label="@string/title_activity_bar_chart_navigation"
            android:theme="@style/AppTheme.NoActionBar" />
